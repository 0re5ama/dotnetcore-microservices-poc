    "DefaultConnection": "User ID=lab_user;Password=lab_pass;Database=lab_netmicro_policy;Host=postgres;Port=5432"
  },
  "RabbitMqOptions" : {
    "Host" : "rabbitmq",
    "Port" : 5672
