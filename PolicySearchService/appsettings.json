{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "spring" : {
    "application" : {
      "name" : "PolicySearchService"
    }
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://eureka-server:8761/eureka",
      "validateCertificates":  false
    },
    "instance" : {
      "appName" : "PolicySearchService",
      "port" : "5060",
      "hostName" : "dotnet-policy-search-service"
    }
  },
  "ElasticSearchOptions": {
    "Connection": "http://elasticsearch:9200"
  },
  "RabbitMqOptions" : {
    "Host" : "rabbitmq",
    "Port" : 5672
  }
}
