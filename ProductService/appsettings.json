{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Settings": {
    "UseInMemoryDatabase": true
  },
  "AllowedHosts": "*",
  "spring" : {
    "application" : {
      "name" : "ProductService"
    }
  },
  "eureka" : {
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://eureka-server:8761/eureka",
      "validateCertificates":  false
    },
    "instance" : {
      "appName" : "ProductService",
      "port" : "5030",
      "hostName" : "dotnet-product-service"
    }
  }
}
